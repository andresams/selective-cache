<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="adminhtml.cache.container">
            <action method="addButton">
                <argument name="buttonId" xsi:type="string">flush_invalidated_only</argument>
                <argument name="data" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Flush Invalidated Only</item>
                    <item name="onclick" xsi:type="string">https://www.maxpronko.com/</item>
                    <item name="class" xsi:type="string">primary flush-cache-magento</item>
                </argument>
            </action>
        </referenceBlock>
    </body>
</page>
